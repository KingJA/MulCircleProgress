<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MulCircleProgress">
        <attr name="progressShape">
            <flag name="circle" value="0"/>
            <flag name="square" value="1"/>
        </attr>
        <attr name="progressRadius" format="dimension"/>
        <attr name="progressWidth" format="dimension"/>
        <attr name="progressDividerWidth" format="dimension"/>
        <attr name="progressBlackgroudColor" format="reference|color"/>
        <attr name="progressProgressColor" format="reference|color"/>
        <attr name="progressDividerColor" format="reference|color"/>
    </declare-styleable>
</resources>